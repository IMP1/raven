def down_from(int n) {
    print(n)
    if (n > 0) down_from(n-1)
}

down_from(5)



def fibonacci(int n) {
    if (n <= 1) {
        return n
    } else {
        return fibonacci(n - 2) + fibonacci(n - 1)
    }
}

for (int i = 0; i < 20; i = i + 1) {
    print(i)
    print(" => ")
    print(fibonacci(i))
    print("\n")
}



def is_even(int n) bool {
    if (n == 0)
        return TRUE
    else
        return is_odd(n-1)
}

def is_odd(int n) bool {
    if (n == 0)
        return FALSE
    else
        return is_even(n-1)
}

test is_even(4)
test is_odd(13)


def up_to(int n) {
    def count (int a, int b) {
        print(a)
        print("\n")
        if (a < b) {
            count(a + 1, b)
        }
    }
    count(1, n)
}

