def down_from(int n) {
    print(n)
    if (n > 0) down_from(n-1)
}

down_from(5)



def fibonacci(int n) {
    if (n <= 1) {
        return n
    } else {
        return fibonacci(n - 2) + fibonacci(n - 1)
    }
}

for (int i = 0; i < 20; i = i + 1) {
    print(i)
    print(" => ")
    print(fibonacci(i))
    print("\n")
}



