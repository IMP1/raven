def triangle (int x) int {
    if (x < 1)
        return 0

    if (x == 1) {
        return 1
    } else {
        return x + triangle(x-1)
    }
}

test triangle(1) == 1
test triangle(2) == 3
test triangle(3) == 6
test triangle(4) == 10

def square (int x) int {
    test x == 3
    int a = triangle(x)
    test x == 3
    int b = triangle(x-1)
    return a + b
}

print(square(3))
print("\n")

